#  Environment
NODE_ENV=development
ENVIRONMENT=dev

# Version Info
APP_VERSION=1.0.0
VITE_VERSION=1.0.0

# Frontend
FRONTEND_PORT=3001
VITE_FE_PORT=5173

# Backend
BACKEND_PORT=5001
PORT=5001

# PostgreSQL
POSTGRES_DB=crypto_place
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-database-password
