# Frontend Environment Variables Example
# Copy this file to .env.development, .env.qas, or .env.production and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:5001
VITE_API_HOST=/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_JWT_EXPIRATION=24h
VITE_REFRESH_TOKEN_KEY=CRYPTO-PLACE-REFRESH-TOKEN
VITE_TOKEN_EXPIRED=1

# WebSocket
VITE_WS_HOST=localhost:3001

# CoinGecko API
VITE_COINGECKO_API_KEY=your_coingecko_api_key

# Feature Flags
VITE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=false

# Application Settings
VITE_DEFAULT_LANGUAGE=zh-TW
VITE_ITEMS_PER_PAGE=20
VITE_VERSION=1.0.0
VITE_FE_PORT=5173

# TeamRoster System
VITE_TEAM_ROSTER_URL=http://your-team-roster-url/
VITE_TEAM_ROSTER_TOKEN=your_team_roster_token

# Account Project System
VITE_ACCPRO_URL=http://your-accpro-url/
VITE_ACCPRO_TOKEN=your_accpro_token

# System API
VITE_SYSTEM_URL=http://your-system-url/
VITE_SYSTEM_TOKEN=your_system_token

# ENBG Workplace
VITE_ENBG_URL=https://your-enbg-url/
VITE_ENBG_TOKEN=your_enbg_token

# Azure AD / Microsoft Authentication
VITE_AZURE_URL=https://login.microsoftonline.com/
VITE_AZURE_TENANT=your_azure_tenant_id
VITE_AZURE_CLIENT_ID=your_azure_client_id
VITE_MICROSOFT_AUTH_URL=https://login.microsoftonline.com/your_tenant_id/oauth2/v2.0/
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id
VITE_REDIRECT_URL_BASE=https://your-redirect-url/callback

# Supervisors (comma-separated employee IDs)
VITE_SUPERVISORS=11111111,22222222
