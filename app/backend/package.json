{
  "name": "crypto-place-backend",
  "version": "2.0.0",
  "description": "NestJS Backend for Crypto Place application",
  "author": "Your Name",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "prisma generate --no-engine && nest build",
    "build:dev": "prisma generate && nest build",
    "build:prod": "prisma generate --no-engine && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "lint:format": "npm run lint:fix && npm run format",
    "start": "nest start",
    "start:dev": "prisma generate --no-engine && nest start --watch",
    "start:dev:full": "prisma generate && nest start --watch",
    "start:debug": "prisma generate --no-engine && nest start --debug --watch",
    "start:debug:full": "prisma generate && nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "npm run start:dev",
    "dev:full": "npm run start:dev:full",
    "prisma:generate": "prisma generate",
    "prisma:generate:prod": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:push": "prisma db push",
    "postinstall": "prisma generate --no-engine",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e-spec\\.ts$ --testPathIgnorePatterns=.*\\.integration\\.spec\\.ts$",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.19",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^6.10.1",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "prisma": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.4.19",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}