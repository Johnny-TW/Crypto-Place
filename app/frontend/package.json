    "lint-staged": "^16.1.2",
    "postcss": "^8.5.2",
    "prettier": "^3.5.0",
    "prettier-eslint": "^16.4.2",
    "react-scan": "^0.3.4",
    "sweetalert2": "^11.16.0",
