{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "start": "pnpm dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,scss,json}\"",
    "lint:format": "npm run lint:fix && npm run format",
    "type-check": "echo 'Type check skipped (no tsconfig.json)'",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "scan": "react-scan",
    "scan:fix": "react-scan --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
