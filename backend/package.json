{
  "name": "crypto-place-backend",
  "version": "2.0.0",
  "description": "NestJS Backend for Crypto Place application",
  "author": "Your Name",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@9.14.0",
  "scripts": {
    "build": "prisma generate --no-engine && nest build",
    "build:dev": "prisma generate && nest build",
    "build:prod": "prisma generate --no-engine && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "lint:format": "npm run lint:fix && npm run format",
    "start:debug": "nest start",
    "start:dev": "prisma generate && nest start --watch",
    "start:dev:full": "prisma generate && nest start --watch",
    "start:dev:local": "NODE_ENV=development prisma generate && nest start --watch",
    "start:qas": "NODE_ENV=qas prisma generate && nest start --watch",
    "start:prod": "node dist/src/main.js",
    "start": "prisma generate --no-engine && nest start --debug --watch",
    "start:debug:full": "prisma generate && nest start --debug --watch",
    "dev": "npm run start:dev",
    "dev:full": "npm run start:dev:full",
    "dev:local": "npm run start:dev:local",
    "dev:qas": "npm run start:qas",
    "dev:docker": "echo '使用 Docker 開發請執行: docker-compose up'",
    "docker:dev": "docker compose up",
    "docker:qas": "docker compose -f docker-compose.qas.yml up",
    "docker:prod": "docker compose -f docker-compose.prod.yml up",
    "prisma:generate": "prisma generate",
    "prisma:generate:prod": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:seed:dev": "prisma generate && prisma db seed",
    "db:reset-seed": "prisma migrate reset --force && prisma db seed",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e-spec\\.ts$ --testPathIgnorePatterns=.*\\.integration\\.spec\\.ts$",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.4",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.19",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^6.10.1",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.0.1",
    "cache-manager-fs-hash": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "express-session": "^1.18.2",
    "form-data": "^4.0.4",
    "googleapis": "^169.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "openai": "^5.10.2",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.4.19",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/form-data": "^2.5.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.0",
    "@types/passport-azure-ad": "^4.3.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.2",
    "pg": "^8.16.3",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.1.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed-simple.ts"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "overrides": {
    "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
    "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
    "multer@>=1.4.4-lts.1 <2.0.2": ">=2.0.2",
    "tmp@<=0.2.3": ">=0.2.4"
  }
}
